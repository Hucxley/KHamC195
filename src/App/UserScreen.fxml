<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="900.0" style="-fx-background-color: #212121;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="App.UserScreenController">
   <children>
      <Label fx:id="txtHeader" prefHeight="54.0" prefWidth="900.0" style="-fx-background-color: #0091ea;" text="Home" textFill="WHITE">
         <font>
            <Font name="Roboto" size="24.0" />
         </font>
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
      <Button fx:id="btnViewAll" alignment="CENTER" contentDisplay="CENTER" layoutX="390.0" layoutY="254.0" mnemonicParsing="false" onAction="#handleViewAllButton" style="-fx-background-color: #0091ea; -fx-background-radius: 0;" text="View Appointments" textFill="WHITE" />
      <Button fx:id="btnAddNew" alignment="CENTER" contentDisplay="CENTER" layoutX="390.0" layoutY="290.0" mnemonicParsing="false" onAction="#handleAddNewButton" prefHeight="25.0" prefWidth="120.0" style="-fx-background-color: #0091ea; -fx-background-radius: 0;" text="New Appointment" textFill="WHITE" />
      <Button fx:id="btnLogoutUser" alignment="CENTER" contentDisplay="CENTER" layoutX="390.0" layoutY="325.0" mnemonicParsing="false" onAction="#handleLogoutUserButton" prefHeight="25.0" prefWidth="120.0" style="-fx-background-color: #0091ea; -fx-background-radius: 0;" text="Logout User" textFill="WHITE" />
   </children>
</AnchorPane>
